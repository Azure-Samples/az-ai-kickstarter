{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },

    "useAiSearch": {
      "value": "${USE_AI_SEARCH=false}"
    },
    "useAuthentication": {
      "value": "${USE_AUTHENTICATION=false}"
    },
    "useExistingAzureOpenAi": {
      "value": "${USE_EXISTING_AZURE_OPENAI=false}"
    },
    "useExistingAiSearch": {
      "value": "${USE_EXISTING_AI_SEARCH=false}"
    },

    "frontendExists": {
      "value": "${SERVICE_FRONTEND_RESOURCE_EXISTS=false}"
    },
    "backendExists": {
      "value": "${SERVICE_BACKEND_RESOURCE_EXISTS=false}"
    },

    "authTenantId": {
      "value": "${AZURE_AUTH_TENANT_ID}"
    },
    "azurePrincipalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "authClientAppId": {
      "value": "${AZURE_CLIENT_APP_ID}"
    },
    "authClientSecret": {
      "value": "${AZURE_CLIENT_APP_SECRET}"
    },

    "azureOpenAiName": {
      "value": "${AZURE_OPENAI_NAME}"
    },
    "azureOpenAiEndpoint": {
      "value": "${AZURE_OPENAI_ENDPOINT}"
    },
    "azureOpenAiApiVersion": {
      "value": "${AZURE_OPENAI_API_VERSION}"
    },
    "executorAzureOpenAiDeploymentName": {
      "value": "${EXECUTOR_AZURE_OPENAI_DEPLOYMENT_NAME}"
    },
    "utilityAzureOpenAiDeploymentName": {
      "value": "${UTILITY_AZURE_OPENAI_DEPLOYMENT_NAME}"
    },

    "azureAiSearchName": {
      "value": "${AZURE_AI_SEARCH_NAME}"
    },
    "azureazureAiSearchResourceGroupName": {
      "value": "${AZURE_AI_SEARCH_RESOURCE_GROUP_NAME}"
    },
    "azureAiSearchLocation": {
      "value": "${AZURE_AI_SEARCH_LOCATION}"
    },
    "aiSearchSkuName": {
      "value": "${AI_SEARCH_SKU_NAME=basic}"
    }
  }
}
